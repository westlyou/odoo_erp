<odoo>
	<template id="ent_lead_signup_email" name="Email Signup">
		<t t-call="website.layout">
			<form action="/return/lead/signup" method="post">
				<input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
				<div class="form-group form-field o_website_form_required_custom">
					<label class="col-md-3 col-sm-4 control-label" for="contact_name">Your Email</label>
                    <div class="col-md-7 col-sm-8">
                        <input type="text" class="form-control o_website_form_input" name="email" required="" placehosder="Enter Your Email..."/>
                    </div>
                </div>
                
                <div class="form-group">
                    <div class="col-md-offset-3 col-sm-offset-4 col-sm-8 col-md-7 mt16">
                        <span class="btn btn-primary btn-sm o_website_form_send">Send</span>
                        <span id="o_website_form_result"></span>
                    </div>
                </div>
			</form>
		</t>
	</template>
	
	<template id="ent_lead_agreement_signup" name="Email Signup Lead">
		<t t-call="website.layout">
			<form action="/return/lead/signup" method="post">
				<form action="/return/lead/signup" method="post">
				<input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
				<div class="form-group form-field o_website_form_required_custom">
					<div class="col-md-5 col-sm-4">
                        <input type="text" class="form-control o_website_form_input" name="name" t-att-value="crm_lead_id.name" required="" placehosder="Enter Your Email..."/>
                    </div>
                    <div class="col-md-7 col-sm-8">
                        <input type="text" class="form-control o_website_form_input" name="name" t-att-value="crm_lead_id.name" required="" placehosder="Enter Your Email..."/>
                    </div>
                    
                    <div class="col-md-5 col-sm-4">
                        <input type="text" class="form-control o_website_form_input" name="name" t-att-value="crm_lead_id.lead_website" required="" placehosder="Enter Your Email..."/>
                    </div>
                    <div class="col-md-7 col-sm-8">
                        <input type="text" class="form-control o_website_form_input" name="name" t-att-value="crm_lead_id.email_from" required="" placehosder="Enter Your Email..."/>
                    </div>
                </div>
                
                <div class="form-group">
                    <div class="col-md-offset-3 col-sm-offset-4 col-sm-8 col-md-7 mt16">
                        <span class="btn btn-primary btn-sm o_website_form_send">Send</span>
                        <span id="o_website_form_result"></span>
                    </div>
                </div>
			</form>
			</form>
		</t>
	</template>
</odoo>
