<odoo>
	<record id="crm_case_form_view_oppor_inherit_custom_season_mk" model='ir.ui.view'>
		<field name="name">crm.lead.view.form.inherit.custom.season.mk</field>
		<field name="model">crm.lead</field>
		<field name="inherit_id" ref="indimedi_crm.crm_case_form_view_oppor_inherit"/>
		<field name="arch" type="xml">
			<xpath expr="//page[@name='agreement_job_description']//field[@name='job_description_ids']/form//field[@name='permanent_hour_selection']" position="after">
				<field name="perm_season_date_from"/>
				<field name="perm_season_date_to"/>
			</xpath>
			<xpath expr="//page[@name='agreement_job_description']//field[@name='job_description_ids']/form//field[@name='task_tobe_done']" position="after">
				<field name="task_to_be_done2"/>
				<field name="task_to_be_done3"/>
				<field name="task_to_be_done4"/>
			</xpath>
			<xpath expr="//field[@name='phone']" position="after">
				<field name="lead_ext"/>
			</xpath>
			<xpath expr="//field[@name='permanent_hour_selection']" position="after">
				<field name="seasonal_hour"/>
				<field name="seasonal_rate_per_hour"/>
			</xpath>
			<xpath expr="//page[@name='agreement_job_description']//field[@name='job_description_ids']/form//field[@name='s_accounting_ids']" position="attributes">
				<attribute name="invisible">1</attribute>
			</xpath>
			<xpath expr="//page[@name='agreement_job_description']//field[@name='job_description_ids']/form//field[@name='s_tax_id_ids']" position="attributes">
				<attribute name="string">Software Experience</attribute>
			</xpath>
		</field>
	</record>
</odoo>
